<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.DairyDao">
    <select id="findAll" resultType="dairy">
        select * from dairy
    </select>

    <insert id="save">
        insert into dairy(name,time,result,message) values(#{name},#{time},#{result},#{message})
    </insert>
</mapper>
